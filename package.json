{
  "name": "nimiq-map",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "yarn type-check && yarn build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-pattern dist/ --ignore-pattern '*.config.js'",
    "api:generate": "yarn openapi-generator-cli generate -g typescript-fetch -i openapi.yaml -o ./src --additional-properties=typescriptThreePlus=true,legacyDiscriminatorBehavior=false,modelPropertyNaming='camelCase',enumPropertyNaming='original' -o ./src/api/ && yarn build"
  },
  "dependencies": {
    "@fontsource/mulish": "^4.5.7",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@googlemaps/markerclusterer": "^2.0.14",
    "@headlessui/vue": "^1.7.2",
    "@nimiq/vue3-components": "github:nimiq/vue3-components#build/full",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue-router": "^4.1.3",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vue3-google-map": "^0.15.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/google.maps": "^3.50.5",
    "@types/grecaptcha": "^3.0.4",
    "@types/node": "^16.11.45",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.1.0",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "focus-visible": "^5.2.0",
    "google-maps": "^4.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.2",
    "tailwindcss-nimiq-theme": "^0.2.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vite-plugin-rewrite-all": "^1.0.0",
    "vue-tsc": "^1.0.10"
  }
}
